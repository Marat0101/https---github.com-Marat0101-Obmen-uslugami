it }) {\n    const fireEmployee = async () => {\n      const employeeId = props.employee.id\n      const companyId = props.employee.company.id\n      try {\n        await fireEmployeeApi(companyId, employeeId)\n      } catch (err) {\n        console.log(err)\n      } finally {\n        emit('employeeFired')\n      }\n    }\n    return { fireEmployee }\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.employee-group {\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n</style>\n"],"mappings":"AAYA,OAAOA,QAAO,MAAO,iCAAgC;AACrD,SAASC,WAAU,QAAS,kBAAiB;AAC7C,OAAOC,SAAQ,MAAO,kCAAiC;AACvD,SAASC,eAAc,QAAS,8BAA6B;AAE7D,eAAe;EACbC,UAAU,EAAE;IAAEF,SAAS;IAAEF;EAAS,CAAC;EACnCK,OAAO,EAAE;IAAEJ;EAAY,CAAC;EACxBK,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,KAAIA,CAAGL,KAAK,EAAE;IAAEM;EAAK,CAAC,EAAE;IACtB,MAAMC,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC/B,MAAMC,UAAS,GAAIR,KAAK,CAACC,QAAQ,CAACQ,EAAC;MACnC,MAAMC,SAAQ,GAAIV,KAAK,CAACC,QAAQ,CAACU,OAAO,CAACF,EAAC;MAC1C,IAAI;QACF,MAAMZ,eAAe,CAACa,SAAS,EAAEF,UAAU;MAC7C,EAAE,OAAOI,GAAG,EAAE;QACZC,OAAO,CAACC,GAAG,CAACF,GAAG;MACjB,UAAU;QACRN,IAAI,CAAC,eAAe;MACtB;IACF;IACA,OAAO;MAAEC;IAAa;EACxB;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}